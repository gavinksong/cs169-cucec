<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, shrink-to-fit=no, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Student Navigation|Cucec</title>

    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

    <!-- Custom CSS -->
    <link href="../../assets/stylesheets/students.scss" rel="stylesheet">

</head>

<body>

<div id="wrapper">

    <!-- Sidebar -->
    <div id="sidebar-wrapper">
        <ul class="sidebar-nav">
            <li class="sidebar-brand">
                <a href="navigation.html.erb">
                    Student Name <!--Insert student name here-->
                </a>
            </li>
            <li>
                <a href="#">Dashboard</a>
            </li>
            <li>
                <a href="#">Points</a>
            </li>
            <li>
                <a href="#">Logout</a>
            </li>
        </ul>
    </div>
    <!-- /#sidebar-wrapper -->

    <!-- Page Content -->
    <div id="page-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">



                    <!-- Student Navigation Tabs -->
                    <div class="row animatedParent">

                        <div class="col-xs-6 col-sm-4 col-md-4">
                            <div class="service-box">
                                <div class="service-desc">
                                    <h5>Listening</h5>
                                    <p>
                                        Listen to English recordings.
                                    </p>
                                    <a href= <%= listening_students_path %> class="btn btn-default">Listen</a>
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-6 col-sm-4 col-md-4">
                            <div class="service-box">
                                <div class="service-desc">
                                    <h5>Speaking</h5>
                                    <p>
                                        Improve your speaking skills.
                                    </p>
                                    <a href=<%= speaking_students_path %> class="btn btn-default">Speak</a>
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-6 col-sm-4 col-md-4">
                            <div class="service-box">
                                <div class="service-desc">
                                    <h5>Reading</h5>
                                    <p>
                                        Compare English and Chinese texts.
                                    </p>
                                    <a href=<%= reading_students_path %>  class="btn btn-default">Read</a>
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-6 col-sm-4 col-md-4">
                            <div class="service-box">
                                <div class="service-desc">
                                    <h5>Writing</h5>
                                    <p>
                                        Practice your English writing skills.
                                    </p>
                                    <a href=<%= writing_students_path %>  class="btn btn-default">Write</a>
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-6 col-sm-4 col-md-4">
                            <div class="service-box">
                                <div class="service-desc">
                                    <h5>Watching</h5>
                                    <p>
                                        Watch English movies and videos.
                                    </p>
                                    <a href=<%= watching_students_path %> class="btn btn-default">Watch</a>
                                </div>
                            </div>
                        </div>

                        <div class="col-xs-6 col-sm-4 col-md-4">
                            <div class="service-box">
                                <div class="service-desc">
                                    <h5>Chatting</h5>
                                    <p>
                                        Talk to an American mentor through
                                        instant messaging.
                                    </p>
                                    <a href=<%= chatting_students_path %>  class="btn btn-default">Chat</a>
                                </div>
                            </div>
                        </div>


                    </div>
                    <!-- End of Navigation tabs-->
                    <br>

                    <p><code>Go to navigation</code></p>
                    <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Toggle menu</a>
                </div>
            </div>
        </div>
    </div>
    <!-- /#page-content-wrapper -->

</div>
<!-- /#wrapper -->

<!-- jQuery library -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>

<!-- Menu Toggle Script -->
<script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
</script>

<!--</body>-->

<!--</html>-->
